<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <deployment>
        <exclusions>
            <!--<module name="org.hibernate" slot="main" />-->
        </exclusions>
        <dependencies>
            <module name="com.mysql" />
            <module name="org.primefaces" slot="6.RELEASE" meta-inf="export"/>
            <module name="net.jasperreports" slot="6.1.0.RELEASE"/>
            <module name="org.springframework" slot="5.0.2.RELEASE">
                <imports>
                    <include path="META-INF**" />
                    <include path="org**" />
                </imports>
            </module>
        </dependencies>
    </deployment>
</jboss-deployment-structure>

